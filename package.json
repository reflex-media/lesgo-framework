{
  "name": "lesgo",
  "version": "0.6.1",
  "description": "Core framework for lesgo node.js serverless framework.",
  "main": "./src/index.js",
  "author": "Sufiyan Rahmat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reflex-media/lesgo-framework/issues"
  },
  "homepage": "https://github.com/reflex-media/lesgo-framework#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reflex-media/lesgo-framework.git"
  },
  "files": [
    "src",
    "bin"
  ],
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run test",
    "lint": "eslint '**/*.{js,ts,jsx,tsx}' --quiet",
    "lint-fix": "eslint '**/*.{js,ts,jsx,tsx}' --fix"
  },
  "bin": {
    "lesgo-scripts": "./bin/lesgo-scripts.sh"
  },
  "engines": {
    "node": "12.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "aws-sdk": "^2.541.0",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.4.0",
    "@firebase/app": "^0.6.12",
    "data-api-client": "^1.1.0",
    "firebase-admin": "^9.3.0",
    "jsonwebtoken": "^8.5.1",
    "memcached-elasticache": "^1.1.1",
    "nanoid": "^3.1.16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "npm run lint",
      "git add"
    ]
  }
}
